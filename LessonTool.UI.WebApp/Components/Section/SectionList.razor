@inherits BaseComponent<SectionList>

<div class="d-flex flex-column justify-content-center align-items-center mx-3">
    @for(int i = 0; i < Sections.Count; i++)
    {
        int index = i;
        <div class="mb-3">
            <SectionItem Section="@Sections[index]" SectionIndex="@index" />
        </div>
    }
</div>